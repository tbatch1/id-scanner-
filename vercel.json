{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/demo/checkout",
      "dest": "/frontend/checkout.html"
    },
    {
      "src": "/checkout.html",
      "dest": "/frontend/checkout.html"
    },
    {
      "src": "/",
      "dest": "/frontend/checkout.html"
    }
  ]
}

      "source": "/",
      "destination": "/admin/data-center.html"
    }
  ],
  "headers": [
    {
      "source": "/payment-gateway.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=*, microphone=*, geolocation=*"
        },
        {
          "key": "Feature-Policy",
          "value": "camera *; microphone *; geolocation *"
        }
      ]
    },

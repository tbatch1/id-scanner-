{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/payment-gateway.html",
      "destination": "/frontend/payment-gateway-auto.html"
    },
    {
      "source": "/scanner.html",
      "destination": "/frontend/scanner.html"
    },
    {
      "source": "/checkout.html",
      "destination": "/frontend/checkout.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/checkout.html"
    }
  ],
  "headers": [
    {
      "source": "/payment-gateway.html",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "camera=*, microphone=*, geolocation=*"
        },
        {
          "key": "Feature-Policy",
          "value": "camera *; microphone *; geolocation *"
        }
      ]
    },
    {
      "source": "/scanner.html",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "camera=*, microphone=*, geolocation=*"
        },
        {
          "key": "Feature-Policy",
          "value": "camera *; microphone *; geolocation *"
        }
      ]
    }
  ]
}

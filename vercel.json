    },
    {
      "source": "/admin/marketing",
      "destination": "/api"
    },
    {
      "source": "/admin/webhooks/(.*)",
      "destination": "/api"
    },
    {
      "source": "/payment-gateway.html",
      "destination": "/frontend/payment-gateway-auto.html"
    },
    {
      "source": "/payment-gateway-stable.html",
      "destination": "/frontend/payment-gateway-stable.html"
    },
    {
      "source": "/payment-gateway-verify.html",
      "destination": "/frontend/id-scan.html"

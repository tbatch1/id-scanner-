{
  "crons": [
    {
      "path": "/api/cron/retention",
      "schedule": "30 3 * * *"
    },
    {
      "path": "/api/cron/customer-reconcile",
      "schedule": "0 4 * * *"
    },
    {
      "path": "/api/cron/webhooks",
      "schedule": "30 4 * * *"
    },
    {
      "path": "/api/cron/customers",
      "schedule": "0 5 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/admin/marketing.html",
      "destination": "/frontend/admin-marketing.html"
    },
    {
      "source": "/admin/oauth.html",
      "destination": "/frontend/admin-oauth.html"
    },
    {
      "source": "/admin/status",
      "destination": "/api"
    },
    {
      "source": "/admin/outlets",
      "destination": "/api"
    },
    {
      "source": "/admin/refresh",
      "destination": "/api"
    },
    {
      "source": "/admin/scans",
      "destination": "/api"
    },
    {
      "source": "/admin/pending/(.*)",
      "destination": "/api"
    },
    {
      "source": "/admin/transactions",
      "destination": "/api"
    },
    {
      "source": "/admin/sales",
